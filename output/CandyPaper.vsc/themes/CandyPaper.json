  "semanticHighlighting": true,
  "semanticTokenColors": {
    "class": "#2828cc",
    "enum": "#2828cc",
    "interface": "#2828cc",
    "struct": "#2828cc",
    "typeParameter": "#2828cc",
    "type": "#2828cc",
    "property": "#7a28cc",
    "property.static": "#c030c0",
    "enumMember": "#c030c0",
    "decorator": "#806000",
    "event": "#7a28cc",
    "macro": "#806000",
    "label": "#2828cc",
    "comment": "#567200",
    "string": "#007272",
    "keyword": "#008020",
    "number": "#c05430",
    "regexp": "#007272",
    "*.deprecated": {
      "fontStyle": "strikethrough"
    }
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "string.quoted.docstring.multi"
      ],
      "settings": {
        "foreground": "#567200"
      }
    },
    {
      "name": "Comment Tags and Other Special Items",
      "scope": [
        "comment storage",
        "keyword.other.documentation",
        "keyword.other.important",
        "meta.shebang",
        "punctuation.definition.string.heredoc.delimiter"
      ],
      "settings": {
        "foreground": "#806000"
      }
    },
    {
      "name": "Comment Tag Values",
      "scope": [
        "comment entity.name.type",
        "comment variable",
        "comment source"
      ],
      "settings": {
        "foreground": "#406080"
      }
    },
    {
      "name": "Literal Constants",
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.language",
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#c05430"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "support.constant",
        "constant.other.caps"
      ],
      "settings": {
        "foreground": "#9a2e2e"
      }
    },
    {
      "name": "Structure Like Items (Class, Interface, Enum, etc.)",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class"
      ],
      "settings": {
        "foreground": "#2828cc"
      }
    },
    {
      "name": "Instance Properties",
      "scope": [
        "support.type.property-name",
        "variable.other.object.property",
        "variable.other.property",
        "meta.record.identifier"
      ],
      "settings": {
        "foreground": "#7a28cc"
      }
    },
    {
      "name": "Enum Members",
      "scope": [
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#c030c0"
      }
    },
    {
      "name": "Annotations",
      "scope": [
        "punctuation.definition.annotation",
        "storage.type.annotation",
        "entity.name.function.decorator"
      ],
      "settings": {
        "foreground": "#806000"
      }
    },
    {
      "name": "Macros",
      "scope": [
        "keyword.control.directive",
        "punctuation.definition.attribute",
        "punctuation.brackets.attribute"
      ],
      "settings": {
        "foreground": "#806000"
      }
    },
    {
      "name": "Labels",
      "scope": [
        "entity.name.label",
        "variable.other.label"
      ],
      "settings": {
        "foreground": "#2828cc"
      }
    },
    {
      "name": "Tags",
      "scope": [
        "entity.name.tag",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#008020"
      }
    },
    {
      "name": "Attribute Names",
      "scope": [
        "entity.other.attribute-name",
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#7a28cc"
      }
    },
    {
      "name": "Invalid Items",
      "scope": [
        "invalid"
      ],
      "settings": {
        "foreground": "#e61818"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage",
        "entity.name.command",
        "keyword.operator.new"
      ],
      "settings": {
        "foreground": "#008020"
      }
    },
    {
      "name": "Primitive Types",
      "scope": [
        "storage.type.primitive",
        "support.type.primitive",
        "entity.name.type.numeric"
      ],
      "settings": {
        "foreground": "#008020"
      }
    },
    {
      "name": "Reserved Variables",
      "scope": [
        "variable.language"
      ],
      "settings": {
        "foreground": "#008020"
      }
    },
    {
      "name": "Global Variables",
      "scope": [
        "variable.other.global",
        "variable.other.anchor",
        "variable.other.alias"
      ],
      "settings": {
        "foreground": "#9a2e2e"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#406080"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#007272"
      }
    },
    {
      "name": "String: Interpolation Punctuations",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.definition.interpolation"
      ],
      "settings": {
        "foreground": "#2828cc"
      }
    },
    {
      "name": "String: Interpolation Items",
      "scope": [
        "string variable"
      ],
      "settings": {
        "foreground": "#406080"
      }
    },
    {
      "name": "String: Unquoted",
      "scope": [
        "string.unquoted"
      ],
      "settings": {
        "foreground": "#406080"
      }
    },
    {
      "name": "Hyperlinks",
      "scope": [
        "variable.parameter.url",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#0066cc"
      }
    },
    {
      "name": "Markup: Bold Text",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Italic Text",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup: Bold and Italic Text",
      "scope": [
        "markup.bold markup.italic",
        "markup.italic markup.bold"
      ],
      "settings": {
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Markup: Strikethrough Text",
      "scope": [
        "markup.strikethrough"
      ],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Markup: Underline Text",
      "scope": [
        "markup.underline"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup: Headings",
      "scope": [
        "punctuation.definition.heading",
        "markup.heading"
      ],
      "settings": {
        "foreground": "#008020"
      }
    },
    {
      "name": "Markup: List Punctuations",
      "scope": [
        "punctuation.definition.list"
      ],
      "settings": {
        "foreground": "#008020"
      }
    },
    {
      "name": "Markup: Raw Blocks, Code Blocks and Quote Blocks",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code",
        "markup.raw",
        "markup.quote"
      ],
      "settings": {
        "foreground": "#007272"
      }
    },
    {
      "name": "Markup: Link Titles and Descriptions",
      "scope": [
        "string.other.link.title",
        "string.other.link.description"
      ],
      "settings": {
        "foreground": "#007272"
      }
    }
  ]
}