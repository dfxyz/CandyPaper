set bg=light
hi clear
let g:colors_name = "CandyPaper"
let g:terminal_ansi_colors = [
<%_ for (const ansiColor of ansiColors) { _%>
  \ "#<%= ansiColor.hex %>",
<%_ } _%>
  \]

<%_ for (const [name, group] of Object.entries(groups)) { _%>
hi clear <%= name %>
hi <%= name %>
  <%_ if (group.fg) { _%>
  \ guifg=#<%= group.fg.hex %>
  <%_ } _%>
  <%_ if (group.bg) { _%>
  \ guibg=#<%= group.bg.hex %>
  <%_ } _%>
  <%_ if (group.gui) { _%>
  \ gui=<%= group.gui %>
  <%_ } _%>
<%_ } _%>
